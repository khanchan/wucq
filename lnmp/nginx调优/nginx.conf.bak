user  nobody;
worker_processes  16; 

error_log /dev/null crit;

events {
    worker_connections  40960;
    use epoll;
}

http {
    include       mime.types;
    default_type  application/octet-stream;

    server_tokens off;
    
    sendfile        on; 
    keepalive_timeout  65; 

    gzip  on; 

    server {
	listen *:80 default;
	server_name _;
	return 444;
	access_log   off;
    }
    include mgt16.conf;
}